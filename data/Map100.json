{"id":4,"name":"EV004","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"Actor2","direction":6,"pattern":1,"characterIndex":6},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["マスター、聞いてよ。"]},{"code":401,"indent":0,"parameters":["凄く面白い話だよ！"]},{"code":401,"indent":0,"parameters":["今日さ、凄く美味しい手作りのラーメンをね。"]},{"code":401,"indent":0,"parameters":["故郷の母に送ろうかと思ったんだ。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["冷めちゃうでしょ。此処は雪国だよ？"]},{"code":401,"indent":0,"parameters":["外出れば、カチカチだ。"]},{"code":401,"indent":0,"parameters":["もっと他に美味しいもの出来なかったの？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["私も、そう思ったんだけどね。"]},{"code":401,"indent":0,"parameters":["他になくてさ。粉があったから麺から作ったんだ。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["あ、もう作ったんだ。"]},{"code":401,"indent":0,"parameters":["その勢いで。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["麺があったから、ではなくて粉があったから。"]},{"code":401,"indent":0,"parameters":["凄く本格的というか。それもうラーメン送るし"]},{"code":401,"indent":0,"parameters":["かないね。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["じゃ～どんどんラーメンが売れて売れてさ。"]},{"code":401,"indent":0,"parameters":["行列が出来るくらい。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["美味しいものを作って届けたいとおもった。"]},{"code":401,"indent":0,"parameters":["その過程で店が出来てるし、口コミも常連も"]},{"code":401,"indent":0,"parameters":["出来てるよね。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["もう１年２年の時間経ってるね。"]},{"code":401,"indent":0,"parameters":["都会で成功してくるぜ！"]},{"code":401,"indent":0,"parameters":["で、その報告みたいだね。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["だから、あの頃の最高のラーメンを送ろうかとね。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["なるほどね～・・・。"]},{"code":401,"indent":0,"parameters":["でも、そのラーメンって。"]},{"code":401,"indent":0,"parameters":["数年前のなんでしょ？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["うんうん、感動の味だよ？"]},{"code":401,"indent":0,"parameters":["マスターも食べてみる？"]},{"code":401,"indent":0,"parameters":["今頃は、麺によく染みてるよ？"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["それもう、ラーメンじゃないね。"]},{"code":401,"indent":0,"parameters":["ラーメンをベースにした毒物だ。"]},{"code":401,"indent":0,"parameters":["危うく、２人食中毒になるところだった。"]},{"code":401,"indent":0,"parameters":["送らない方がいいよ。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["う～ん、まぁでも面白かったでしょ？"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["途中は、ほ～。"]},{"code":401,"indent":0,"parameters":["で、後半笑えないね。"]},{"code":401,"indent":0,"parameters":["それで面白い貴方は、猟奇的だ。"]},{"code":401,"indent":0,"parameters":["愉快犯だ。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["なんだ～、マスター。"]},{"code":401,"indent":0,"parameters":["やっぱり面白かったんじゃないか～。"]},{"code":401,"indent":0,"parameters":["愉快犯だなんて。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["それ愉快なのは自分だけだね。"]},{"code":401,"indent":0,"parameters":["あと水で７時間粘るのはやめてくれ。"]},{"code":401,"indent":0,"parameters":["帰ってくれ。そのまま自首してくれ。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["じゃ、この水代つけておいて。"]},{"code":401,"indent":0,"parameters":["雪国だけに、背筋の凍るような面白い話になっちゃったね。"]},{"code":401,"indent":0,"parameters":["じゃ～、２時間後にくるよ。"]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["もう来なくていい、帰れ。"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":10,"y":6},
